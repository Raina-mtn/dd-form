<view class="asstes-id-wrapper">
  <form-item 
    className="asstes-id-form"
    position="{{position}}"
    label="{{label}}"
    status="{{formData.status}}"
    errors="{{formData.errors}}"
    tooltip="{{tooltip}}"
    requiredMark="{{requiredMark}}"
    validateStatus="{{validateStatus}}"
    help="{{help}}">
    <input disabled="{{disabled}}"  placeholder="请输入内容" onChange="onChange"/>
  </form-item>
  <icon a:if="{{ !disabled }}" class="asstes-id-scanning" type="ScanningOutline" onTap="onScanning" />
</view>