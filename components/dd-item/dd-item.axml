<!-- input输入框 -->
<form-input
  a:if="{{column.el==='input'}}"
  name="{{column.name}}" 
  label="{{column.label}}" 
  position="{{column.position}}" 
  validateStatus="{{column.validateStatus}}" 
  help="{{column.help}}" 
  tooltip="{{column.tooltip}}" 
  required="{{column.required}}" 
  message="{{column.message}}" 
  requiredMark="{{column.requiredMark}}" 
  defaultValue="{{column.defaultValue}}" 
  disabled="{{column.disabled}}" 
  maxLength="{{column.maxLength}}" 
  placeholder="{{column.placeholder||`请输入${column.label}`}}" 
  style="{{column.style}}" 
  allowClear="{{column.allowClear}}" 
  focus="{{column.focus}}" 
  password="{{column.password}}" 
  prefix="{{column.prefix}}" 
  suffix="{{column.suffix}}" 
  confirm-type="{{column.confirmType}}" 
  placeholderClassName="placeholderCustom"
  type="{{column.type}}" 
  ref="handleRef"
  ></form-input>

