  tooltip="{{column.tooltip}}"
  required="{{column.required}}"
  requiredMark="{{column.requiredMark}}"
  validateStatus="{{column.validateStatus}}"
  help="{{help}}"
  onFileListChange="onFileListChange"
  onTap="onFileListChange"
  >
    <form-media
      value="{{value}}"
      column="{{column}}"
      disabled="{{column.disabled}}"
    />
  </form-item>

